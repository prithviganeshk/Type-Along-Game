<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/bob"
    tools:context="lab3.cis542.upenn.edu.typinggame.Scores">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" SCORES"
        android:id="@+id/score_view"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK"
        android:id="@+id/back_button"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/score_view"
        android:layout_alignEnd="@+id/score_view"
        android:layout_alignLeft="@+id/score_view"
        android:layout_alignStart="@+id/score_view" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="         YOUR HIGHEST SCORE IS "
        android:id="@+id/highestscore_view"
        android:layout_below="@+id/score_view"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="43dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="0"
        android:id="@+id/score_textView"
        android:layout_below="@+id/highestscore_view"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="EASY"
        android:id="@+id/textView"
        android:layout_below="@+id/score_textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="EASY BEST"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/highestscore_view"
        android:layout_alignStart="@+id/highestscore_view" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="BEST TIME"
        android:id="@+id/textView3"
        android:layout_toEndOf="@+id/textView2"
        android:layout_alignRight="@+id/score_view"
        android:layout_alignEnd="@+id/score_view"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignLeft="@+id/score_view"
        android:layout_alignStart="@+id/score_view" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="WORST TIME"
        android:id="@+id/textView4"
        android:layout_toEndOf="@+id/textView3"
        android:layout_above="@+id/easy_worsttime"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="no-one"
        android:id="@+id/easy_username"
        android:layout_toStartOf="@+id/score_textView"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/easy_bestime"
        android:layout_alignTop="@+id/easy_username"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/easy_worsttime"
        android:layout_toEndOf="@+id/textView3"
        android:layout_alignTop="@+id/easy_bestime"
        android:layout_toRightOf="@+id/textView3"
        android:layout_marginLeft="50dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="           MEDIUM"
        android:id="@+id/textView5"
        android:layout_toEndOf="@+id/easy_username"
        android:layout_toStartOf="@+id/easy_worsttime"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/easy_worsttime"
        android:layout_alignEnd="@+id/easy_worsttime" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="MED BEST"
        android:id="@+id/textView6"
        android:layout_toStartOf="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_toLeftOf="@+id/textView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="BEST TIME"
        android:id="@+id/textView7"
        android:layout_toEndOf="@+id/textView2"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="WORST TIME"
        android:id="@+id/textView8"
        android:layout_toEndOf="@+id/textView3"
        android:layout_alignTop="@+id/textView7"
        android:layout_toRightOf="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="no-one"
        android:id="@+id/med_username"
        android:layout_toStartOf="@+id/textView7"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_alignRight="@+id/textView6"
        android:layout_alignEnd="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/med_bestime"
        android:layout_alignTop="@+id/med_username"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/med_worstime"
        android:layout_toEndOf="@+id/easy_worsttime"
        android:layout_below="@+id/textView8"
        android:layout_toRightOf="@+id/easy_worsttime" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="             HARD"
        android:id="@+id/textView12"
        android:layout_toEndOf="@+id/med_username"
        android:layout_toStartOf="@+id/easy_worsttime"
        android:layout_below="@+id/med_bestime"
        android:layout_toLeftOf="@+id/easy_worsttime" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="HARD BEST"
        android:id="@+id/textView13"
        android:layout_toStartOf="@+id/textView12"
        android:layout_below="@+id/textView12"
        android:layout_toLeftOf="@+id/textView12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="BEST TIME"
        android:id="@+id/textView14"
        android:layout_toEndOf="@+id/textView2"
        android:layout_alignTop="@+id/textView13"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="WORST TIME"
        android:id="@+id/textView15"
        android:layout_toEndOf="@+id/score_textView"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="no-one"
        android:id="@+id/hard_username"
        android:layout_toStartOf="@+id/textView"
        android:layout_below="@+id/textView13"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/hard_bestime"
        android:layout_alignTop="@+id/hard_username"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/hard_worstime"
        android:layout_toEndOf="@+id/textView14"
        android:layout_alignTop="@+id/hard_bestime"
        android:layout_alignLeft="@+id/med_worstime"
        android:layout_alignStart="@+id/med_worstime" />

</RelativeLayout>
